package com.webdev.productsystem.Tours.Tickets.Domain;

import com.webdev.productsystem.Tours.Tickets.Domain.ValueObjects.TicketDate;
import com.webdev.productsystem.Tours.Tickets.Domain.ValueObjects.TicketGate;
import com.webdev.productsystem.Tours.Tickets.Domain.ValueObjects.TicketId;
import com.webdev.productsystem.Tours.Tickets.Domain.ValueObjects.TicketSeat;
import com.webdev.productsystem.Tours.Tour.Domain.ValueObjects.TourBookingId;
import com.webdev.productsystem.Tours.Tour.Domain.ValueObjects.TourCity;
import com.webdev.productsystem.Tours.Tour.Domain.ValueObjects.TourId;
import com.webdev.productsystem.Tours.Tour.Domain.ValueObjects.TourName;
import com.webdev.productsystem.Users.User.Domain.ValueObjects.UserId;

public class Ticket {

    public TicketId ticketId;
    public TicketGate ticketGate;
    public TicketSeat ticketSeat;
    public TicketDate ticketDate;
    public UserId userId;
    public City originCity;
    public City destinyCity;


    public Ticket(TicketId ticketId, TicketGate ticketGate, TicketSeat ticketSeat, TicketDate ticketDate, UserId userId, City originCity, City destinyCity) {
        this.ticketId = ticketId;
        this.ticketGate = ticketGate;
        this.ticketSeat = ticketSeat;
        this.ticketDate = ticketDate;
        this.originCity = originCity;
        this.destinyCity = destinyCity;
        this.userId = userId;
    }

    public static Ticket create(TicketId ticketId, TicketGate ticketGate, TicketSeat ticketSeat, TicketDate ticketDate, UserId userId, City originCity, City destinyCity){
        Ticket ticket = new Ticket(ticketId, ticketGate, ticketSeat, ticketDate, userId, originCity, destinyCity);
        return ticket;
    }



}